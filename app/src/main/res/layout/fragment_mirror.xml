<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anubh.cosmic.mirror">

   <android.support.constraint.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:background="@drawable/inside_banner">

       <TextView
           android:id="@+id/textView50"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/TextView50"
           android:textSize="25sp"
           android:textColor="#FF004D"
           app:layout_constraintTop_toTopOf="parent"
           android:layout_marginTop="8dp"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintVertical_bias="0.037" />

       <ImageButton
           android:id="@+id/imageButton18"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           app:srcCompat="@drawable/mkt_icon_5"
           android:layout_marginTop="8dp"
           app:layout_constraintTop_toBottomOf="@+id/textView50"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintHorizontal_bias="0.074"
           app:layout_constraintVertical_bias="0.051" />

       <ImageButton
           android:id="@+id/imageButton19"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           app:srcCompat="@drawable/ret_icon_5"
           android:layout_marginTop="8dp"
           app:layout_constraintTop_toBottomOf="@+id/textView50"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintHorizontal_bias="0.772"
           app:layout_constraintVertical_bias="0.049" />

       <TextView
           android:id="@+id/textView51"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textColor="#ECFF00"
           android:textSize="15sp"
           android:layout_marginTop="8dp"
           app:layout_constraintTop_toBottomOf="@+id/imageButton18"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintHorizontal_bias="0.47"
           app:layout_constraintVertical_bias="0.03" />
   </android.support.constraint.ConstraintLayout>

</FrameLayout>
