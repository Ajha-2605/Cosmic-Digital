<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anubh.cosmic.oculus">

   <android.support.constraint.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:background="@drawable/oculus_0">

       <TextView
           android:id="@+id/textView44"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/TextView44"
           android:textColor="#DCFE03"
           android:textSize="25sp"
           app:layout_constraintTop_toTopOf="parent"
           android:layout_marginTop="8dp"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintVertical_bias="0.053"
           app:layout_constraintHorizontal_bias="0.428" />

       <TextView
           android:id="@+id/textView45"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="15sp"
           android:textColor="#5D00FD"
           android:layout_marginTop="8dp"
           android:textStyle="bold"
           app:layout_constraintBottom_toBottomOf="parent"
           android:layout_marginBottom="8dp"
           app:layout_constraintTop_toBottomOf="@+id/textView44"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintVertical_bias="0.315" />

       <ImageButton
           android:id="@+id/imageButton12"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           app:srcCompat="@drawable/edu_icon"
           android:layout_marginTop="8dp"
           app:layout_constraintTop_toBottomOf="@+id/textView44"
           android:layout_marginBottom="8dp"
           app:layout_constraintBottom_toTopOf="@+id/textView45"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           android:layout_marginLeft="8dp"
           app:layout_constraintLeft_toLeftOf="parent"
           app:layout_constraintHorizontal_bias="0.197"
           app:layout_constraintVertical_bias="0.388" />

       <ImageButton
           android:id="@+id/imageButton13"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           app:srcCompat="@drawable/ent_icon_10"
           android:layout_marginRight="8dp"
           app:layout_constraintRight_toRightOf="parent"
           app:layout_constraintLeft_toRightOf="@+id/imageButton12"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="8dp"
           app:layout_constraintTop_toBottomOf="@+id/textView44"
           android:layout_marginBottom="8dp"
           app:layout_constraintBottom_toTopOf="@+id/textView45" />
   </android.support.constraint.ConstraintLayout>
</FrameLayout>
